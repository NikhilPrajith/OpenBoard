{
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Content-Security-Policy",
            "value": "default-src 'self'; img-src 'self' https://*.firebase.com; script-src 'self' 'unsafe-inline'; object-src 'none'"
          }
        ]
      }
    ]
  }
  